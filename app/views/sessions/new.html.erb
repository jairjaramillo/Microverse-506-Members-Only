<h1>Login</h1>

<%= form_for :session, url: login_path do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit 'Sign in' %>

    <p>
        Not signed up? <%= link_to 'Signup here!', signup_path %>
    </p>
<% end %>
